	},
	"devDependencies": {
		"concurrently": "^5.2.0",
		"nodemon": "^2.0.4",
		"react-time-format": "0.0.5"
	},
	"dependencies": {
		"axios": "^0.21.1",
		"bcryptjs": "^2.4.3",
		"config": "^3.3.1",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"express-validator": "^6.6.0",
		"gravatar": "^1.8.1",
		"if-env": "^1.0.4",
		"jsonwebtoken": "^8.5.1",
